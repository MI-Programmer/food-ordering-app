generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  auth0Id      String
  email        String
  name         String?
  addressLine1 String?
  city         String?
  country      String?
}
